<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.palmcms.api.code.CodeMapper">

    <select id="selectCodesByCodeGroup" resultType="com.palmcms.api.domain.DTO.CommonCodeDTO">
        select *
        from TO_COMMON_CODE
        where codeGroup = #{codeGroup}
        order by dispOrder
    </select>


</mapper>